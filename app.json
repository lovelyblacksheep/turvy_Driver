{
  "expo": {
    "name": "turvyDriver",
    "slug": "turvyDriver",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/splashscreen_image.png",
      "resizeMode": "cover"
    },
    "icon": "./assets/icon.png",
    "android": {
      "package": "com.turvy.turvydriver",
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/request_alert.mp3"
          ],
          "mode": "production"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.turvy.turvydriver"
    }
  },
  "name": "turvyDriver"
}
